<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JobOrder">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="98dp"
        android:background="@drawable/textview_border"
        android:text="@string/textViewPalvelutTyot"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="195dp"
        android:layout_height="90dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="153dp"
        android:background="@drawable/textview_border"
        android:text="@string/textviewTyoInfo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="269dp"
        android:layout_marginTop="153dp"
        android:contentDescription="@string/imageViewTyo"
        android:src="@drawable/inttisesu_mini"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="268dp"
        android:layout_marginTop="266dp"
        android:contentDescription="@string/imageViewTyo"
        android:src="@drawable/inttisesu_mini"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="195dp"
        android:layout_height="90dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="266dp"
        android:background="@drawable/textview_border"
        android:text="@string/textviewCustomJob"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/round_button"
        android:onClick="profileButtonClicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/profile_pic_icon_foreground"
        android:contentDescription="@string/imageButtonProfileInJobOrder" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="332dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/round_button"
        android:onClick="homeButtonClicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home_pic_icon_foreground"
        android:contentDescription="@string/imageButtonHomeInJobOrder" />

</androidx.constraintlayout.widget.ConstraintLayout>